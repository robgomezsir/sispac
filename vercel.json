{
  "rewrites": [
    { "source": "/welcome", "destination": "/index.html" },
    { "source": "/join", "destination": "/index.html" },
    { "source": "/setup-password", "destination": "/index.html" },
    { "source": "/complete-invite", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/welcome", "destination": "/index.html", "permanent": false },
    { "source": "/join", "destination": "/index.html", "permanent": false },
    { "source": "/setup-password", "destination": "/index.html", "permanent": false },
    { "source": "/complete-invite", "destination": "/index.html", "permanent": false }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
